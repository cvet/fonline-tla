bool CheckCompare(int val1, int operator, int val2)
{
    if (operator== '>') {
        return val1 > val2;
    }
    if (operator== '<') {
        return val1 < val2;
    }
    if (operator== '=') {
        return val1 == val2;
    }
    Game.Log("Unknown operator.");
    return false;
}

bool CheckCompareAnyParam(CCheck check, Critter[] critters)
{
    for (int i = 0, j = critters.length(); i < j; i++) {
        if (CheckCompare(critters[i].GetAsInt(CritterProperty(check.Index)), check.Operator, check.Value)) {
            return true;
        }
    }
    return false;
}

bool CheckCompareAnyVar(CCheck check, Critter[] critters)
{
    for (int i = 0, j = critters.length(); i < j; i++) {
        if (CheckCompare(critters[i].GetAsInt(CritterProperty(check.Index)), check.Operator, check.Value)) {
            return true;
        }
    }
    return false;
}

void CheckAssignVar(Critter cr, CritterProperty prop, int operator, int val2)
{
    int val1 = cr.GetAsInt(prop);
    if (operator== '=') {
        val1 = val2;
    }
    else if (operator== '+') {
        val1 += val2;
    }
    else if (operator== '-') {
        val1 -= val2;
    }
    else if (operator== '*') {
        val1 *= val2;
    }
    else if (operator== '/') {
        val1 /= val2;
    }
    else {
        Game.Log("Unknown operator.");
    }
    cr.SetAsInt(prop, val1);
}

bool CheckChecks(CCheck[] checks, Critter[] critters)
{
    for (int i = 0; i < checks.length(); i++) {
        CCheck check = checks[i];
        switch (check.Type) {
        case CHECK_RANDOM:
            return Game.Random(1, 100) <= check.Value;
        case CHECK_HOUR:
            return CheckCompare(GameTime::GetHour(), check.Operator, check.Value);
        case CHECK_PARAM:
            for (int j = 0; j < critters.length(); j++) {
                if (not CheckCompare(critters[j].GetAsInt(CritterProperty(check.Index)), check.Operator, check.Value)) {
                    return false;
                }
            }
            break;
        case CHECK_PARAM_ANY:
            return CheckCompareAnyParam(check, critters);
        case CHECK_PROPERTY:
            for (int j = 0; j < critters.length(); j++) {
                if (not CheckCompare(critters[j].GetAsInt(CritterProperty(check.Index)), check.Operator, check.Value)) {
                    return false;
                }
            }
            break;
        case CHECK_PROPERTY_ANY:
            return CheckCompareAnyVar(check, critters);
        case CHECK_GVAR: {
            if (not CheckCompare(Game.GetAsInt(GameProperty(check.Index)), check.Operator, check.Value)) {
                return false;
            }
        } break;
        default:
            continue;
        }
    }
    return true;
}

void AssignChecks(CCheck[] checks, Critter[] critters)
{
    for (int i = 0; i < checks.length(); i++) {
        CCheck check = checks[i];
        if (check.Type == ASSIGN_PROPERTY) {
            for (int j = 0; j < critters.length(); j++) {
                CheckAssignVar(critters[j], CritterProperty(check.Index), check.Operator, check.Value);
            }
        }
    }
}

//
// Stored founded encounters
//

#define FOUNDED_ENCOUNTER_LIVE_TIME (Time::Minutes(30)) // 30 real minutes
class CFoundedEncounter
{
    int Index;
    hstring[] LocationPids = {};
    CEncounter Encounter;
    bool IsGenerated;
    synctime ServerTime;
    int PositionX;
    int PositionY;
    ident OwnerId;
    bool OwnerGroup;
    ident LocationId;
    bool IsWater;
    mpos StartHex;
    bool[] DirBusy = {};
    int AskChance;

    CFoundedEncounter()
    {
        Index = 0;
        Encounter = null;
        IsGenerated = false;
        ServerTime = ZERO_SYNCTIME;
        PositionX = 0;
        PositionY = 0;
        OwnerId = ZERO_IDENT;
        OwnerGroup = false;
        LocationId = ZERO_IDENT;
        IsWater = false;
        StartHex = mpos();
        DirBusy.resize(6);
    }
}

CFoundedEncounter[] FoundedEncounters = array<CFoundedEncounter>(1000);
int FoundedEncountersLastIndex = 0;

CFoundedEncounter CreateFoundedEncounter(bool isWater, int x, int y, CEncounter encounter, hstring[] locationPids, ident ownerId, bool ownerGroup)
{
    for (int i = 0, j = FoundedEncounters.length(); i < j; i++) {
        if (FoundedEncounters[i] == null) {
            FoundedEncounters[i] = CFoundedEncounter();
        }
        CFoundedEncounter ge = FoundedEncounters[i];
        if (ge.Index == 0 || Game.SynchronizedTime - ge.ServerTime >= FOUNDED_ENCOUNTER_LIVE_TIME + Time::Minutes(1)) {
            FoundedEncountersLastIndex++;
            ge.Index = FoundedEncountersLastIndex;
            ge.LocationPids = locationPids;
            ge.Encounter = encounter;
            ge.IsGenerated = false;
            ge.ServerTime = Game.SynchronizedTime;
            ge.PositionX = x;
            ge.PositionY = y;
            ge.OwnerId = ownerId;
            ge.OwnerGroup = ownerGroup;
            ge.LocationId = ZERO_IDENT;
            ge.IsWater = isWater;
            for (int k = 0; k < 6; k++) {
                ge.DirBusy[k] = false;
            }
            return ge;
        }
    }

    // Grow buffer and refind
    FoundedEncounters.resize(FoundedEncounters.length() * 2);
    return CreateFoundedEncounter(isWater, x, y, encounter, locationPids, ownerId, ownerGroup);
}

CFoundedEncounter GetFoundedEncounter(int index)
{
    for (int i = 0, j = FoundedEncounters.length(); i < j; i++) {
        CFoundedEncounter ge = FoundedEncounters[i];
        if (ge != null && ge.Index == index) {
            return ge;
        }
    }
    return null;
}

CFoundedEncounter GetNearFoundedEncounter(bool isWater, Critter[] group, int x, int y, int radius)
{
    for (int i = 0, j = FoundedEncounters.length(); i < j; i++) {
        if (FoundedEncounters[i] == null) {
            FoundedEncounters[i] = CFoundedEncounter();
        }
        CFoundedEncounter ge = FoundedEncounters[i];

        if (ge.Index == 0) {
            continue;
        }
        if (ge.IsWater != isWater) {
            continue;
        }
        if (Game.SynchronizedTime - ge.ServerTime >= FOUNDED_ENCOUNTER_LIVE_TIME) {
            continue;
        }
        if (not ge.IsGenerated) {
            continue;
        }
        if (valid(ge.Encounter) && ge.Encounter.IsSpecial && not CheckChecks(ge.Encounter.Checks, group)) {
            continue;
        }
        if (int(math::sqrt(float(POW2(ge.PositionX - x) + POW2(ge.PositionY - y)))) > radius) {
            continue;
        }
        if (group[0].Id == ge.OwnerId) {
            continue;
        }
        if (not valid(Game.GetLocation(ge.LocationId))) {
            continue;
        }

        return ge;
    }
    return null;
}

int GetFreeDir(bool[] dirBysy)
{
    // Try find random
    uint8 dir = Game.Random(0, 5);
    if (not dirBysy[dir]) {
        dirBysy[dir] = true;
        return dir;
    }

    // Try find any free
    for (int i = 0; i < 6; i++) {
        if (not dirBysy[i]) {
            dirBysy[i] = true;
            return i;
        }
    }

    // Any busy
    return Game.Random(0, 5);
}

void RotatePosition(int toDir, int[] positionsDirs)
{
    for (int i = 1, j = positionsDirs.length(); i < j; i++) // Skip Steps
    {
        positionsDirs[i] += toDir;
        if (positionsDirs[i] > 5) {
            positionsDirs[i] -= 6;
        }
    }
}

void MovePositionPoint(Map map, mpos& hex, int[] positionDirs, int pathIndex, int count, int spacing)
{
    int steps = positionDirs[0];
    int pathLen = (positionDirs.length() - 1) / 2;
    count *= steps;
    count %= pathLen;
    if (steps > 1) {
        for (int i = 0; i < spacing; i++) {
            for (int j = 0; j < steps; j++) {
                uint8 dir = positionDirs[1 + pathLen * pathIndex + count + j];
                map.MoveHexByDir(hex, dir, 1);
            }
        }
    }
    else {
        uint8 dir = positionDirs[1 + pathLen * pathIndex + count];
        map.MoveHexByDir(hex, dir, spacing);
    }
}

void ParseEncounterObject(CEncounterObject obj, Map map, mpos hex, int reversDir, Critter[] crittersGrouop, int teamId)
{
    if (obj.ObjectType == ENCOUNTER_OBJECT_CRITTER) {
        if (not map.IsHexMovable(hex)) {
            return;
        }

        int[] props = {CritterProperty::DialogId,
                       obj.DialogId.hash,
                       CritterProperty::BagId,
                       Bags::Empty,
                       CritterProperty::TeamId,
                       teamId,
                       CritterProperty::ReplicationTime,
                       -1};
        Critter npc = map.AddCritter(obj.Pid, hex, reversDir, Tla::GetCritPropsDict(props));
        npc.SetupScript(obj.ScriptName);
        if (not valid(npc)) {
            return;
        }

        for (int i = 0, j = obj.Childs.length(); i < j; i++) {
            CEncounterObject child = obj.Childs[i];
            int count = Game.Random(child.Minimum, child.Maximum);
            if (count > 0) {
                Item item = npc.AddItem(child.Pid, count);
                if (child.InvSlot != CritterItemSlot::Inventory) {
                    npc.ChangeItemSlot(item.Id, child.InvSlot);
                }
            }
        }

        if (obj.IsDead) {
            CritterState::ToDead(npc, Game.Random(0, 1) == 0 ? CritterActionAnim::DeadFront : CritterActionAnim::DeadBack, null);
        }
        else {
            crittersGrouop.insertLast(npc);
        }
    }
    else if (obj.ObjectType == ENCOUNTER_OBJECT_ITEM) {
        Item item = map.AddItem(hex, obj.Pid, 1);
        if (not valid(item)) {
            return;
        }

        if (obj.Childs.length() > 0 && item.Type == ItemType::Container) {
            for (int i = 0, j = obj.Childs.length(); i < j; i++) {
                CEncounterObject child = obj.Childs[i];
                int count = Game.Random(child.Minimum, child.Maximum);
                if (count > 0) {
                    item.AddItem(child.Pid, count, ROOT_CONTAINER_STACK);
                }
            }
        }
    }
    else {
        Game.Log("Unknown object type<" + obj.ObjectType + ">.");
    }
}

void ActionFighting(Critter[] npcL, Critter[] npcR)
{
    for (int i = 0, j = npcL.length(); i < j; i++) {
        Critter nl = npcL[i];
        bool nlIsNpc = !nl.ControlledByPlayer;
        for (int k = 0, l = npcR.length(); k < l; k++) {
            Critter nr = npcR[k];
            if (nlIsNpc) {
                NpcPlanes::AddAttackPlane(nl, 0, nr);
                EnemyStack::AddEnemyToStack(nl, nr.Id);
                CritterState::Wait(nl, Game.Random(4000, 5000));
            }
            if (!nr.ControlledByPlayer) {
                NpcPlanes::AddAttackPlane(nr, 0, nl);
                EnemyStack::AddEnemyToStack(nr, nl.Id);
                CritterState::Wait(nr, Game.Random(4000, 5000));
            }
        }
    }
}

Map GenerateEncounter(CFoundedEncounter fe, Critter[] critters)
{
    CEncounter encounter = fe.Encounter;
    hstring locPid = (valid(encounter) ? encounter.LocPid : EMPTY_HSTRING);
    if (locPid == EMPTY_HSTRING) {
        locPid = fe.LocationPids[Game.Random(0, fe.LocationPids.length() - 1)];
    }

    fe.LocationId = Location::CreateLocation(locPid, ipos(fe.PositionX, fe.PositionY), null).Id;
    if (fe.LocationId == ZERO_IDENT) {
        Game.Log("CreateLocation fail, location pid<" + locPid + ">.");
        return null;
    }

    Location location = Game.GetLocation(fe.LocationId);
    if (not valid(location)) {
        Game.Log("GetLocation fail.");
        return null;
    }
    location.IsEncounter = true;

    // Генерация итемов на всех картах энкаунтера
    for (int i = 0, l = location.GetMapCount(); i < l; i++) {
        Map m = location.GetMapByIndex(i);
        if (valid(m)) {
            EncounterItems::GenerateItems(m, critters);
            if (i > 0) {
                CaveMobs::GenerateCavernEncounter(m, critters);
            }
        }
    }

    Map map = location.GetMapByIndex(0);
    if (not valid(map)) {
        Game.Log("GetMapByIndex fail.");
        return null;
    }

    mpos hexStart;
    if (not Entrance::MapGetEntryCoords(map, 0, 0, hexStart)) {
        Game.Log("Default entry not found.");
        return null;
    }
    fe.StartHex = hexStart;

    if (not valid(encounter)) // Empty map
    {
        fe.IsGenerated = true;
        return map;
    }

    Critter leader = critters[0];
    Critter[][] crittersGrouops = array<array<Critter>>(encounter.Groups.length());

    for (int i = 0, j = encounter.Groups.length(); i < j; i++) {
        int groupNum = encounter.Groups[i];

        if (groupNum == GROUP_Player) {
            crittersGrouops[i] = array<Critter>();
            Critter[] crGroup = crittersGrouops[i];
            crGroup.resize(critters.length());
            for (int k = 0, l = critters.length(); k < l; k++) {
                crGroup[k] = critters[k];
            }
            crittersGrouops[i] = crGroup;
            continue;
        }

        if (groupNum >= GROUP_MAX) {
            Game.Log("Invalid group number.");
            continue;
        }

        CEncounterGroup group = EncounterGroups[groupNum];
        int ratio = Game.Random(encounter.RatioMin[i], encounter.RatioMax[i]);

        int distance = group.Distance;
        if (distance == 0) {
            distance = 7 + leader.Perception + leader.PerkCautiousNature * 3;
        }

        int teamId = group.TeamNum;
        if (teamId == 0) {
            teamId = Teams::Mob0 + i;
        }

        int position = group.Position;
        int spacing = group.Spacing;
        if (position == POSITION_SURROUNDING) {
            spacing = leader.Perception + leader.PerkCautiousNature * 3;
        }

        int[] positionDirs = PositionDirs[position];
        int steps = positionDirs[0];
        int dirsCount = 0;

        int freeDir = GetFreeDir(fe.DirBusy);
        if (freeDir != 0) {
            RotatePosition(freeDir, positionDirs);
        }
        int reversDir = freeDir + 3;
        if (reversDir > 5) {
            reversDir -= 6;
        }

        // Get start point for group
        mpos[] points = {hexStart, hexStart};
        if (position != POSITION_NONE) {
            mpos hex = points[0];
            map.MoveHexByDir(points[0], freeDir, position != POSITION_SURROUNDING ? distance : spacing);
            map.GetHexInPath(hex, points[0], 0.0f, 0); // Check for barrier
            points[1] = points[0];
            MovePositionPoint(map, points[1], positionDirs, 1, 0, spacing);
        }

        for (int k = 0, l = group.Objects.length(); k < l; k++) {
            CEncounterObject obj = group.Objects[k];

            if (not CheckChecks(obj.Checks, critters)) {
                continue;
            }

            int otherDistance = obj.DistanceLength;
            int count = obj.RatioValue * ratio / 100;
            if (count == 0) {
                count = 1;
            }

            mpos hexOldPos = hexStart;
            for (int n = 0; n < count; n++) {
                int posX, posY;
                mpos hexPos;
                if (otherDistance > 0 || position == POSITION_NONE) {
                    posX = hexStart.x + Game.Random(-5, 5);
                    posY = hexStart.y + Game.Random(-5, 5);
                    hexPos = mpos(posX, posY);
                    map.MoveHexByDir(hexPos, freeDir, otherDistance > 0 ? otherDistance : distance);
                }
                else {
                    int pathIndex = dirsCount % 2;
                    posX = points[pathIndex].x + (position == POSITION_SURROUNDING ? Game.Random(-3, 0) : 0);
                    posY = points[pathIndex].y + (position == POSITION_SURROUNDING ? Game.Random(-3, 0) : 0);
                    hexPos = mpos(posX, posY);
                    MovePositionPoint(map, points[pathIndex], positionDirs, pathIndex, dirsCount, spacing);
                    dirsCount++;
                }

                map.GetHexInPath(hexOldPos, hexPos, 0.0f, 0); // Check for barrier
                hexOldPos = hexPos;
                if (crittersGrouops[i] == null) {
                    crittersGrouops[i] = array<Critter>();
                }
                ParseEncounterObject(obj, map, hexPos, reversDir, crittersGrouops[i], teamId);
            }
        }
    }

    for (int i = 0, j = encounter.Actions.length(); i < j; i += 3) {
        int fromGroup = encounter.Actions[i];
        int toGroup = encounter.Actions[i + 1];
        int action = encounter.Actions[i + 2];

        if (action == ACTION_FIGHTING && fromGroup < crittersGrouops.length() && toGroup < crittersGrouops.length()) {
            ActionFighting(crittersGrouops[fromGroup], crittersGrouops[toGroup]);
        }
    }

    fe.IsGenerated = true;
    return map;
}

