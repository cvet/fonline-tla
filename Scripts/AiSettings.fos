void AI_TrySayCombatText(Critter npc, int textType)
{
    if (!npc.IsAlive()) {
        return;
    }

    uint ai = npc.AiId;
    if (AI_MESS_CHANCE(ai) < Game.Random(1, 500)) {
        return;
    }

    uint start = 0, end = 0;
    if (textType >= CombatText::RunAway && textType <= CombatText::Miss) {
        start = AIGeneralMsg_[AI_GENERAL_TYPE(ai) * 8 + textType * 2];
        end = AIGeneralMsg_[AI_GENERAL_TYPE(ai) * 8 + textType * 2 + 1];
    }
    if (textType >= CombatText::HitHead && textType <= CombatText::HitGroin) {
        textType -= 10;
        start = AIBodyMsg_[AI_BODY_TYPE(ai) * 16 + textType * 2];
        end = AIBodyMsg_[AI_BODY_TYPE(ai) * 16 + textType * 2 + 1];
    }
    else {
        return;
    }
    if (start <= end) {
        npc.SayMsg(SAY_NORM_ON_HEAD, TextPackName::Combat, Game.Random(start, end) * 10);
    }
}

