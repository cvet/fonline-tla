uint GetNearServerTime(uint16 year, uint16 month, uint16 day, uint16 hour, uint16 minute, uint16 second)
{
    int jump = 0; // 1 - years, 2 - months, 3 - days

    if (year == 0) {
        year = Game.Year;
        jump = 1;
    }
    if (month == 0) {
        month = Game.Month;
        jump = 2;
    }
    if (day == 0) {
        day = Game.Day;
        jump = 3;
    }

    uint time = Game.DateToServerTime(year, month, day, hour, minute, second);

    while (time <= Game.GetServerTime()) {
        switch (jump) {
        case 0:
            return Game.GetServerTime() + 1; // Time expired, call faster
        case 1:
            year++;
            break;
        case 2:
            month++;

            if (month > 12) {
                month = 1;
                year++;
            }
            break;
        case 3:
            day++;

            if (day > 28 && day > Time::GetDaysInMonth(year, month)) {
                day = 1;
                month++;

                if (month > 12) {
                    month = 1;
                    year++;
                }
            }
            break;
        default:
            break;
        }

        time = Game.DateToServerTime(year, month, day, hour, minute, second);
    }

    return time;
}

