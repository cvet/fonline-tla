class CCheck
{
    int Type;
    int Index;
    int Operator;
    int Value;

    CCheck()
    {
        Type = 0;
        Index = 0;
        Operator = 0;
        Value = 0;
    }

    void Set(int type, int index, int operator, int value)
    {
        Type = type;
        Index = index;
        Operator = operator;
        Value = value;
    }
};

class CEncounterObject
{
    int ObjectType;
    hstring Pid; // Proto id
    hstring DialogId;
    CritterInitFunc ScriptName;
    int DistanceLength;             // Distance from players
    CEncounterObject[] Childs = {}; // Items in inventory or container
    CCheck[] Checks = {};
    // Only for critter
    int RatioValue;
    bool IsDead;
    // Only for item
    int Minimum;
    int Maximum;
    CritterItemSlot InvSlot;

    CEncounterObject()
    {
        ObjectType = 0;
        Pid = EMPTY_HSTRING;
        DialogId = EMPTY_HSTRING;
        @ScriptName = null;
        DistanceLength = 0;
        RatioValue = 0;
        IsDead = false;
        Minimum = 0;
        Maximum = 0;
        InvSlot = CritterItemSlot::Inventory;
    }

    CEncounterObject NewCheck(int type, int index, int operator, int value)
    {
        Checks.grow(1);
        Checks.last() = CCheck();
        Checks.last().Set(type, index, operator, value);
        return this;
    }

    CEncounterObject Dialog(hstring dialog)
    {
        DialogId = dialog;
        return this;
    }
    CEncounterObject Script(CritterInitFunc script)
    {
        @ScriptName = script;
        return this;
    }
    CEncounterObject Distance(int distance)
    {
        DistanceLength = distance;
        return this;
    }
    CEncounterObject CheckRandom(int value)
    {
        NewCheck(CHECK_RANDOM, 0, 0, value);
        return this;
    }
    CEncounterObject CheckParam(int index, int operator, int value)
    {
        NewCheck(CHECK_PARAM, index, operator, value);
        return this;
    }
    CEncounterObject CheckParamAny(int index, int operator, int value)
    {
        NewCheck(CHECK_PARAM_ANY, index, operator, value);
        return this;
    }
    CEncounterObject CheckProperty(int index, int operator, int value)
    {
        NewCheck(CHECK_PROPERTY, index, operator, value);
        return this;
    }
    CEncounterObject CheckPropertyAny(int index, int operator, int value)
    {
        NewCheck(CHECK_PROPERTY_ANY, index, operator, value);
        return this;
    }
    CEncounterObject CheckGVar(int index, int operator, int value)
    {
        NewCheck(CHECK_GVAR, index, operator, value);
        return this;
    }
    CEncounterObject CheckHour(int operator, int value)
    {
        NewCheck(CHECK_HOUR, 0, operator, value);
        return this;
    }
    CEncounterObject Ratio(int ratio)
    {
        RatioValue = ratio;
        return this;
    }
    CEncounterObject Dead(bool dead)
    {
        IsDead = dead;
        return this;
    }
    CEncounterObject Slot(CritterItemSlot slot)
    {
        InvSlot = slot;
        return this;
    }

    CEncounterObject AddItem(hstring pid, int min, int max, CritterItemSlot slot)
    {
        CEncounterObject new_child = CEncounterObject();
        Childs.insertLast(new_child);
        new_child.Pid = pid;
        new_child.Minimum = min;
        new_child.Maximum = max;
        new_child.InvSlot = slot;
        return this;
    }
};

class CEncounterGroup
{
    int TeamNum;
    int Position;
    int Spacing;
    int Distance;
    CEncounterObject[] Objects = {};

    CEncounterGroup()
    {
        TeamNum = 0;
        Position = 0;
        Spacing = 0;
        Distance = 0;
    }

    CEncounterObject NewObject()
    {
        Objects.grow(1);
        Objects.last() = CEncounterObject();
        return Objects.last();
    }

    CEncounterObject AddCritter(hstring pid)
    {
        CEncounterObject obj = NewObject();
        obj.ObjectType = ENCOUNTER_OBJECT_CRITTER;
        obj.Pid = pid;
        return obj;
    }

    CEncounterObject AddItem(hstring pid)
    {
        CEncounterObject obj = NewObject();
        obj.ObjectType = ENCOUNTER_OBJECT_ITEM;
        obj.Pid = pid;
        return obj;
    }
};

class CEncounter
{
    int Chance;
    int StrNum;
    hstring LocPid;
    bool IsSpecial;
    int[] Groups = {};
    int[] RatioMin = {};
    int[] RatioMax = {};
    int[] Actions = {};
    CCheck[] Checks = {};

    CEncounter()
    {
        Chance = 0;
        StrNum = 0;
        LocPid = EMPTY_HSTRING;
        IsSpecial = false;
    }

    CEncounter AddGroup(int group, int ratioMin, int ratioMax)
    {
        Groups.grow(1);
        RatioMin.grow(1);
        RatioMax.grow(1);
        Groups.last() = group;
        RatioMin.last() = ratioMin;
        RatioMax.last() = ratioMax;
        return this;
    }

    CEncounter Fighting(int fromGroup, int toGroup)
    {
        Actions.insertLast(fromGroup);
        Actions.insertLast(toGroup);
        Actions.insertLast(ACTION_FIGHTING);
        return this;
    }

    CEncounter NewCheck(int type, int index, int operator, int value)
    {
        Checks.grow(1);
        Checks.last() = CCheck();
        Checks.last().Set(type, index, operator, value);
        return this;
    }

    CEncounter LocationPid(hstring locationPid)
    {
        LocPid = locationPid;
        return this;
    }
    CEncounter Special(bool special)
    {
        IsSpecial = special;
        return this;
    }
    CEncounter CheckRandom(int value)
    {
        NewCheck(CHECK_RANDOM, 0, 0, value);
        return this;
    }
    CEncounter CheckParam(int index, int operator, int value)
    {
        NewCheck(CHECK_PARAM, index, operator, value);
        return this;
    }
    CEncounter CheckParamAny(int index, int operator, int value)
    {
        NewCheck(CHECK_PARAM_ANY, index, operator, value);
        return this;
    }
    CEncounter CheckProperty(int index, int operator, int value)
    {
        NewCheck(CHECK_PROPERTY, index, operator, value);
        return this;
    }
    CEncounter CheckPropertyAny(int index, int operator, int value)
    {
        NewCheck(CHECK_PROPERTY_ANY, index, operator, value);
        return this;
    }
    CEncounter CheckGVar(int index, int operator, int value)
    {
        NewCheck(CHECK_GVAR, index, operator, value);
        return this;
    }
    CEncounter CheckHour(int operator, int value)
    {
        NewCheck(CHECK_HOUR, 0, operator, value);
        return this;
    }
    CEncounter AssignProperty(int index, int operator, int value)
    {
        NewCheck(ASSIGN_PROPERTY, index, operator, value);
        return this;
    }
};

class CEncounterTable
{
    hstring[] LocationPids = {};
    CEncounter[] Encounters = {};

    void AddLocationPid(hstring locationPid)
    {
        LocationPids.insertLast(locationPid);
    }

    CEncounter AddEncounter(int chance, int strNum)
    {
        Encounters.grow(1);
        Encounters.last() = CEncounter();
        Encounters.last().Chance = chance;
        Encounters.last().StrNum = strNum;
        return Encounters.last();
    }
};

class CZone
{
    int Difficulty;
    int Terrain;
    int MorningChance;
    int AfternoonChance;
    int NightChance;
    CEncounterTable Table;

    CZone()
    {
        Difficulty = 0;
        Terrain = 0;
        MorningChance = 0;
        AfternoonChance = 0;
        NightChance = 0;
        Table = null;
    }
};

//
// Zone
//

void SetZone(int zx, int zy, int table, int difficulty, int terrain, int morningChance, int afternoonChance, int nightChance)
{
    CZone zone = CZone();
    zone.Table = EncounterTables[table];
    zone.Difficulty = difficulty;
    zone.Terrain = terrain;
    zone.MorningChance = morningChance;
    zone.AfternoonChance = afternoonChance;
    zone.NightChance = nightChance;

    if (terrain != TERRAIN_Water) {
        WorldmapGround[zy * ZONE_COUNT_X + zx] = zone;
    }
    else {
        WorldmapWater[zy * ZONE_COUNT_X + zx] = zone;
    }
}

void SetZone(int zx, int zy, int table, int difficulty, int terrain, int chance)
{
    SetZone(zx, zy, table, difficulty, terrain, chance, chance, chance);
}

//
// Tables
//

void AddTableToAllTables(CEncounterTable globalTable)
{
    for (int i = 0; i < TABLE_MAX; i++) {
        if (i != TABLE_Ocean1_O && i != TABLE_Ocean2_O) {
            CEncounterTable table = EncounterTables[i];

            for (int j = 0; j < globalTable.LocationPids.length(); j++) {
                table.LocationPids.grow(1);
                table.LocationPids.last() = globalTable.LocationPids[j];
            }

            for (int j = 0; j < globalTable.Encounters.length(); j++) {
                table.Encounters.grow(1);
                table.Encounters.last() = globalTable.Encounters[j];
            }
        }
    }
}

